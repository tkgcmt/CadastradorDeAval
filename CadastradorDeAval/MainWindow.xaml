                <Separator Height="5" Margin="0" Style="{DynamicResource SimpleSeparator}"/>
                <MenuItem Header="Ver em/Exportar para pdf..." Style="{DynamicResource SimpleMenuItem}"/>
                <MenuItem Header="Ver/Exportar árvore em pdf..." Style="{DynamicResource SimpleMenuItem}"/>
            </MenuItem>
        </Menu>
