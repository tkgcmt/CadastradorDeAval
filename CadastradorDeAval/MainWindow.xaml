                    <Label x:Name="AvalTituloLbl" Content="csv" VerticalContentAlignment="Top" HorizontalContentAlignment="Center" Background="#FF0F0D0D" MinHeight="25" MinWidth="60" Foreground="#FFDADADA" Margin="0,0,0,25" Grid.Column="1" RenderTransformOrigin="0.284,0.6" Height="25" VerticalAlignment="Bottom" />
                    <TextBox x:Name="AvalTituloTxt" Style="{DynamicResource SimpleTextBox}" TextWrapping="NoWrap" Text="{Binding Path=Caminho}" Visibility="Visible" Margin="69,0,200,25" Grid.Column="1" Height="23" VerticalAlignment="Bottom" Grid.ColumnSpan="2" ToolTip="{Binding Text, ElementName=AvalTituloTxt, UpdateSourceTrigger=PropertyChanged}" TabIndex="3"/>
                    <!-- Descrição -->
                    <Label x:Name="AvalDescrLbl" Content="Descrição" Height="19"  Style="{DynamicResource SimpleLabel}" VerticalContentAlignment="Center" VerticalAlignment="Top" HorizontalContentAlignment="Center" Background="Black" Foreground="#FFDADADA" Margin="0,41,0,0" Grid.Row="3" Grid.Column="1" />
