                    <Button x:Name="AvalFeedbackNegDel" Content="Apagar" Style="{DynamicResource SimpleButton}" Background="#FFD4D0C8" ClipToBounds="True" MinWidth="60" Margin="0,2,200,0" Grid.Column="2" Grid.Row="2" Height="25" VerticalAlignment="Top" HorizontalAlignment="Right" Width="60" ToolTip="Apaga o Arquivo de Feedback Negativo" Click="AvalFeedbackNegDel_Click" TabIndex="9" />
