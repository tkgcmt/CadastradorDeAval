                    <!-- === Feedback Avaliação === -->
                    <Label x:Name="AvalFeedbackLbl" Content="Feedback &#xA;Avaliação"  Style="{DynamicResource SimpleLabel}" Margin="0,10,0,46" Background="Black" Foreground="#FFDADADA"  HorizontalContentAlignment="Center" Grid.Column="1" Grid.Row="1" Grid.RowSpan="2"/>
