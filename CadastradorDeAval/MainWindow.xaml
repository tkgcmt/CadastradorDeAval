                    <Button x:Name="AvalFeedbackPosBtn" Content="Localizar" Style="{DynamicResource SimpleButton}" HorizontalAlignment="Left" Width="61" Grid.Column="2" Grid.Row="1" Height="25" VerticalAlignment="Top" Click="AvalFeedbackPosBtn_Click" ToolTip="Localizar Arquivo de Feedback Positivo" TabIndex="4" />
                    <TextBox x:Name="AvalFeedbackPosTxt" Style="{DynamicResource SimpleTextBox}" Margin="61,0,258,0" Grid.Column="2" Grid.Row="1" Height="25" VerticalAlignment="Top" Text="{Binding Path=PosFeed}" ToolTip="{Binding Text, ElementName=AvalFeedbackPosTxt, UpdateSourceTrigger=PropertyChanged}" MaxLines="1" TextWrapping="NoWrap" TextChanged="AvalFeedbackPosTxt_TextChanged" TabIndex="5" />
                    <Button x:Name="AvalFeedbackPosDel" Content="Apagar" Style="{DynamicResource SimpleButton}"  Margin="0,0,200,0" MinWidth="60" Grid.Column="2" Grid.Row="1" Height="25" VerticalAlignment="Top" HorizontalAlignment="Right" Width="60" ToolTip="Apaga o Arquivo de Feedback Positivo" Click="AvalFeedbackPosDel_Click" TabIndex="6"/>
                    <!-- Feedback Neg -->
                    <Button x:Name="AvalFeedbackNegBtn" Content="Localizar" Style="{DynamicResource SimpleButton}" Margin="0,2,0,0" HorizontalAlignment="Left" Width="61" Grid.Column="2" Grid.Row="2" Height="25" VerticalAlignment="Top" Click="AvalFeedbackNegBtn_Click" ToolTip="Localizar Arquivo de Feedback Negativo" TabIndex="7" />
