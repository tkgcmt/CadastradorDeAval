                <MenuItem Header="Salvar" Style="{DynamicResource SimpleMenuItem}"/>
                <MenuItem Header="Salvar Como..." Style="{DynamicResource SimpleMenuItem}"/>
                <Separator Height="5" Margin="0" Style="{DynamicResource SimpleSeparator}"/>
                <MenuItem Header="Fechar" Style="{DynamicResource SimpleMenuItem}"/>
            </MenuItem>
            <MenuItem x:Name="EditarMenu" Header="Editar" Margin="0" Style="{DynamicResource SimpleMenuItem}" Height="20" VerticalAlignment="Top" Background="#FF0F0D0D" Foreground="#FFDEDEDE" BorderThickness="0,0,1,1" BorderBrush="#FFDEDEDE">
                <MenuItem Header="Desfazer" Style="{DynamicResource SimpleMenuItem}"/>
                <MenuItem Header="Refazer" Style="{DynamicResource SimpleMenuItem}"/>
                <MenuItem Header="Copiar" Style="{DynamicResource SimpleMenuItem}"/>
                <MenuItem Header="Colar" Style="{DynamicResource SimpleMenuItem}"/>
                <Separator Height="5" Margin="0" Style="{DynamicResource SimpleSeparator}"/>
                <MenuItem Header="dummy" Style="{DynamicResource SimpleMenuItem}"/>
            </MenuItem>
            <MenuItem x:Name="VisualizarMenu" Header="Visualizar" Margin="0" Style="{DynamicResource SimpleMenuItem}" Height="20" VerticalAlignment="Top" Background="#FF0F0D0D" Foreground="#FFDEDEDE" BorderThickness="0,0,1,1" BorderBrush="#FFDEDEDE">
