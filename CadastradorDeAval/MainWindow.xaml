        <TabControl x:Name="BarraAbaTC" Style="{DynamicResource SimpleTabControl}" BorderBrush="#FF0F0D0D" Background="#FF0F0D0D" d:LayoutOverrides="LeftMargin, RightMargin" Margin="0,20,0,0">
