                    <TextBlock x:Name="AvalAjuda" Margin="20,41,10,14" TextWrapping="NoWrap" Text="Campo de ajuda." Background="#FF3A3A3A" Foreground="#FFDEDEDE" Grid.Row="3" Grid.Column="3" Grid.ColumnSpan="2" Grid.RowSpan="2"/>
                    <!-- Estatísticas -->
                    <TextBlock x:Name="AvalStat" Margin="20,10,10,10" TextWrapping="NoWrap" Foreground="#FFDEDEDE" Grid.Column="3" Grid.ColumnSpan="2"><Run Text="Disciplinas: "/><LineBreak/><Run/><LineBreak/><Run Text="Questões: "/></TextBlock>
                </Grid>
