                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="159*"/>
                        <ColumnDefinition Width="70*"/>
                        <ColumnDefinition Width="490*"/>
                        <ColumnDefinition Width="171*"/>
                        <ColumnDefinition Width="69*"/>
                    </Grid.ColumnDefinitions>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="97*"/>
                        <RowDefinition Height="40*"/>
                        <RowDefinition Height="55*"/>
                        <RowDefinition Height="127*"/>
                        <RowDefinition Height="158*"/>
                    </Grid.RowDefinitions>
                    <!-- === Botões === -->
                    <!-- Excluir Avaliação -->
                    <Button x:Name="AvalDel" Content="Excluir Avaliação" Style="{DynamicResource SimpleButton}" Margin="10,0,20,15" Grid.Row="4" Height="25" VerticalAlignment="Bottom" Click="AvalDel_Click" TabIndex="11"/>
                    <!-- Nova Avaliação -->
                    <Button x:Name="AvalNovo" Content="Nova Avaliação" Style="{DynamicResource SimpleButton}" Margin="10,62,20,0" Height="25" VerticalAlignment="Top" Click="AvalNovo_Click" TabIndex="2"/>
